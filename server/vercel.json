{
    "version":2,
    "name":"PClub-backend",
    "builds":[
        {
        "src":"server/wsgi.py",
        "use":"@vercel/python",
        "config":{
            "maxLambdaSize":"30mb",
            "runtime":"python3.9"
        }
    },
    {
        "src":"build_files.sh",
        "use":"@vercel/static-build",
        "config":{
            "distDir":"staticfiles"
        }
    }
],
    "routes":[
    {
        "src":"/static/(.*)",
        "dest":"static/$1"
    },
    {
        "src":"/(.*)",
        "dest":"server/wsgi.py"
    }
]
}